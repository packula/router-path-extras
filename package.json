    "@snout/router-path": "^0.6.0",
    "@snout/tsconfig": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codecov": "^3.0.2",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-node": "^11.1.0",
